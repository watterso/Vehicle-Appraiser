  "manifest_version": 2,

  "name": "Ebay Car Value",
  "description": "Augments Ebay listings",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
  "content_scripts": [
    {
      "matches": ["http://motors.shop.ebay.com/Cars-Trucks-/*"],
      "css": ["styles.css"],
      "js": ["jquery.js", "script.js"]
    }
  ],